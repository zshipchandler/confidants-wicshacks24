{
    "manifest_version": 3,
    "name": "Confidant",
    "version": "0.1",
    "icons": {
        "128": "confidants.png"
     },
    "description": "web extension for the Confidant app",

    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],

    "action": {
        "default_title": "Confidant",
        "default_popup": "popup/popup.html"
      }

}
